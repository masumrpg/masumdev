{
  "name": "@masumdev/rn-qrcode-scanner",
  "version": "1.0.3",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/masumrpg"
  },
  "homepage": "https://docs.masum.cloud/docs/category/-qrcode-pack",
  "scripts": {
    "build": "bob build"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.lib.json",
          "tsc": "../../node_modules/.bin/tsc"
        }
      ]
    ]
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/module/index.js",
  "types": "./dist/typescript/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typescript/index.d.ts",
      "import": "./dist/module/index.js",
      "require": "./dist/commonjs/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "react-native",
    "scroll",
    "scroll-to-hide",
    "hide",
    "android",
    "ios",
    "react",
    "qrcode",
    "react-native-qrcode",
    "qrcode-scanner",
    "react-native-qrcode-scanner",
    "react-native-qrcode-scanner-android",
    "react-native-qrcode-scanner-ios",
    "react-native-qrcode-scanner-react-native",
    "react-native-qrcode-scanner-android",
    "react-native-qrcode-scanner-ios",
    "react-native-qrcode-scanner-react-native",
    "mobile"
  ],
  "author": "Masum",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masumrpg/masumdev.git",
    "directory": "libs/rn-qrcode-scanner"
  },
  "dependencies": {
    "qrcode": "^1.5.4"
  },
  "peerDependencies": {
    "react": ">=18.3.1",
    "react-native": ">=0.76.9",
    "react-native-svg": ">=15.8.0",
    "expo": ">=50.0.0",
    "expo-camera": ">=14.0.0",
    "expo-haptics": ">=12.8.0",
    "expo-router": ">=3.4.0",
    "@expo/vector-icons": ">=14.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": false
    },
    "expo": {
      "optional": false
    }
  }
}
